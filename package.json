{
  "name": "tagfs-vscode",
  "displayName": "TagFS Integration",
  "description": "VS Code extension to interact with TagFS for hierarchical file tagging.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "publisher": "Raghu Boggavarapu",
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tagfs.init",
        "title": "TagFS: Initialize",
        "category": "TagFS"
      },
      {
        "command": "tagfs.listtags",
        "title": "TagFS: Show all tags",
        "category": "TagFS"
      },
      {
        "command": "tagfs.addtag",
        "title": "TagFS: Add new tag",
        "category": "TagFS"
      },
      {
        "command": "tagfs.editfiletags",
        "title": "TagFS: Add/Remove tags to file",
        "category": "TagFS"
      },
      {
        "command": "tagfs.searchbytag",
        "title": "TagFS: Search for files with tags",
        "category": "TagFS"
      },
      {
        "command": "tagfs.showfiletags",
        "title": "TagFS: Show tags for file",
        "category": "TagFS"
      }
    ]
  },
  "categories": [
    "Other"
  ],
  "scripts": {},
  "dependencies": {}
}